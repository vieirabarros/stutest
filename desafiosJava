/*
//Desafio 2: Faça um algoritmo que leia a idade(em anos, meses e dias) e mostre-a expressa em dias
//Date: 10/25/2019
//Author: Filipe Barros

import java.util.Scanner;

public class Desafio2

{
	public static void main(String[] args) {
	Scanner input = new Scanner(System.in);		

    System.out.println("\nAnos:");
	int anos=input.nextInt();

    System.out.println("\nMeses:");
	int meses=input.nextInt();
    
    System.out.println("\nDias:");
	int dias=input.nextInt();
    

    int idadeDias=anos*365+meses*30+dias;
    System.out.println("\nVoce tem "+idadeDias+" Dias de vida");
	   
}

}

*/

/*

//Desafio 3: Faça um algoritmo que leia a idade(em anos, meses e dias) e mostre-a expressa em dias
//Date: 10/25/2019
//Author: Filipe Barros

import java.util.Scanner;

public class Desafio3

{
	public static void main(String[] args) {
	Scanner input = new Scanner(System.in);		

    System.out.println("\nDigite o valor do Raio:");
	double raio=input.nextDouble();
	
    if (raio>=0){
        double diametro=2*raio;
        double circunferencia=2*3.14*raio;
        double area=3.14*Math.pow(raio,2);
        
        System.out.println("\nDiametro: "+diametro+"\nCircunferencia: "+circunferencia+"\nArea: "+area);
    }
    
    else {System.out.println("\nValor deve ser um numero maior ou igual a 0");}
}
}

*/


